<div class="container-xxl mt-5 mb-5 contact">

  <h1>掲載店舗様ログイン</h1>
  <%= form_with(url: shop_login_path, scope: :session)  do|f| %>
      <div class="mb-3">
        <%= f.label :email, 'Email' %>
        <%= f.email_field :email, class: 'form-control' %>
      </div>

      <div class="mb-3">
        <%= f.label :password, 'Password' %>
        <%= f.password_field :password, class: 'form-control' %>
      </div>

      <%= f.submit 'Log in', class: 'btn btn-primary w-100' %>
    <% end %>
</div>
